enum ItemType : ubyte { Undefined = 0, Bin = 1, Box, Product, Kit, Container, Location}
enum BinFlags : ubyte { None = 0, Missing = 1, Damaged = 2, Reserved = 4, HasSale = 8}


struct BinItem {
    ItemID: ulong;
    ShipmentID: ulong;
    Count: uint;    
    Type: ItemType = Product;        
}

table Bin {
    Items: [BinItem];
    Type: ItemType;
    Flags: BinFlags = None;
    Code: string;
    Subtype: uint;
    SaleID: ulong;
    
}